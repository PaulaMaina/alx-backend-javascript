{
  "name": "unittests_in_js",
  "version": "1.0.0",
  "author": "Pauline Maina",
  "license": "MIT",
  "description": "This project contains tasks pertaining the basics of unit tests in JavaScript",
  "scripts": {
    "lint": "./node_modules/.bin/eslint",
    "lint-all": "./node_modules/.bin/eslint [0-9]*.js",
    "lint-fix-all": "./node_modules/.bin/eslint --fix [0-9]*.js",
    "test": "node ./node_modules/mocha/bin/mocha --require babel-register --exit",
    "dev": "nodemon --exec babel-node --presets babel-preset-env ./full_server/server.js ./database.csv"
  },
  "dependecies": {
    "chai-http": "^4.3.0",
    "express": "^4.17.1",
    "semistandard": "^17.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^28.8.3",
    "mocha": "^10.7.3",
    "nodemon": "^3.1.4",
    "request": "^2.88.0",
    "sinon": "^7.5.0"
}
